def nil()
	{}
in

def is_nil(lst)
	length(lst) = 0
in

def cons(x, lst)
	{x, lst}
in

def head(lst)
	lst[0]
in

def tail(lst)
	lst[1]
in

def loop(tuple, idx)
	if idx < 0
		then nil()
		else cons(tuple[idx], loop(tuple, idx - 1))
in

def linked_list_of_tuple(tuple)
	loop(tuple, length(tuple) - 1)
in

linked_list_of_tuple({1, 2, 3, 4, 5})
